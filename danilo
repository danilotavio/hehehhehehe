
let color;

let circuloX;

let circuloY;

function setup() {

createCanvas(600, 400);

background("darkblue");

color = ["#A3AF2B", "#CC9DD4", "rgb(195,150,66)"];
            

circuloX = [300, 200, 300];

circuloY = [200, 200, 300];

}

function draw() {

for(let contador in circuloX){

fill(color[contador]);

circle(circuloX[contador], circuloY[contador], 50);

}

}
